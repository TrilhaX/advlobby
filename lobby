    end
end

function autoSummon()
    while getgenv().autoSummon == true do
        local ohString1 = "EventClover"
        local ohString2 = "gems10"

        game:GetService("ReplicatedStorage").endpoints.client_to_server.buy_from_banner:InvokeServer(ohString1, ohString2)
    end
end


